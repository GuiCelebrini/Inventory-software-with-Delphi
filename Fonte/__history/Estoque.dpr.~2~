program Estoque;

uses
  Vcl.Forms,
  UMain in 'UMain.pas' {FormMain},
  UAbout in 'UAbout.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormMain, FormMain);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
